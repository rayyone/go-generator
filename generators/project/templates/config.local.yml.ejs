database:
  driver: "postgres"
  name: "rayyone"
  user: ""
  password: ""
  host: "127.0.0.1"
  port: "5432"
  logLevel: "Info" # Silent, Error, Warn, Info. https://gorm.io/docs/logger.html#Log-Levels

app:
  package: "<%= appModName %>"
  env: "local"
  httpClientDebug: true
  enableCron: true
  apiDebug: true
  ginDebugMode: "debug" # debug, release, test
  basicAuthUser: admin
  basicAuthPass: secret123

server:
  port: "<%= project.projectPort %>"

jwt:
  expirationTime: "648000"
  secret: "secret_for_local"

externalAPI:

googleAPI:
  clientIds:
    - "XXXXX-XXXXX.apps.googleusercontent.com"
    - "YYYYY-YYYYY.apps.googleusercontent.com"

appleAPI:
  default: "sub.domain.com"
  clientID: "sub.domain.com"
  teamID: "XXXXXXXXXX"
  keyID: "XXXXXXXXXX"
  p8: |-
    -----BEGIN PRIVATE KEY-----
    PRIVATE_KEY_GOES_HERE...........................................
    ................................................................
    ........
    -----END PRIVATE KEY-----

others:
  verifyAccountDomain: "https://rayyone.com"

Sentry:
  DSN: "https://hello@999.ingest.sentry.io/99999"

storage:
  default: "s3"
  s3:
    bucket: "rayyone-data-dev"
    region: "ap-southeast-1"
    acl: "public-read"

mail:
  default: "smtp"
  smtp:
    host: "smtp.gmail.com"
    port: 587
    email: "support@rayyone.com"
    password: ""
    name: "Rayyone Support"
