{
  "name": "ry-go-generator",
  "description": "Yeoman Code Generator for Rayyone Golang. Inspired by Loopback 4 CLI generator",
  "version": "0.0.1",
  "author": "Rayyone Ltd.",
  "keywords": [
    "rayyone",
    "code generator",
    "golang generator",
    "template",
    "code template",
    "cli generator",
    "yeoman"
  ],
  "license": "MIT",
  "main": "bin/cli-main.js",
  "bin": {
    "rygo": "./bin/cli-main.js"
  },
  "repository": {
    "url": "https://github.com/rayyone/go-generator",
    "type": "git"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "type": "commonjs",
  "files": [
    "bin",
    "lib",
    "generators",
    "intl",
    ".yo-rc.json"
  ],
  "scripts": {
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx}",
    "lint": "eslint ./src --ext .js,.jsx --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "camelcase-keys": "^7.0.2",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "debug": "^4.3.3",
    "fs-extra": "^10.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^7.0.4",
    "inquirer-autocomplete-prompt": "^1.4.0",
    "lodash": "^4.17.21",
    "minimatch": "^3.0.5",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "natural-compare": "^1.4.0",
    "pacote": "^12.0.3",
    "pluralize": "^8.0.0",
    "regenerate": "^1.4.2",
    "semver": "^7.3.5",
    "slash": "^4.0.0",
    "spdx-license-list": "^6.4.0",
    "stringify-object": "^3.3.0",
    "strong-globalize": "^6.0.5",
    "tabtab": "^3.0.2",
    "terminal-link": "^3.0.0",
    "ts-morph": "^13.0.3",
    "typescript": "^4.5.5",
    "unicode-10.0.0": "^0.7.5",
    "update-notifier": "^5.1.0",
    "url-slug": "^3.0.3",
    "validate-npm-package-name": "^3.0.0",
    "write-file-atomic": "^4.0.0",
    "yeoman-environment": "^3.9.1",
    "yeoman-generator": "^5.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": ">=10",
    "mem-fs": "^2.2.1",
    "mem-fs-editor": "^9.4.0",
    "prettier": "^2.5.1"
  },
  "config": {
    "templateDependencies": {}
  },
  "lint-staged": {
    "generators/**/*.{js,jsx}": ["npx eslint --fix', 'npx prettier --write"]
  }
}
